extends base
block content
  h2 ALL BOOKINGS
  if(bookings.length!=0)
    .signup-parent
      table.bookings
        tr
          th Plan ID
          th User ID
          th Booking ID
          th Time of Delivery
          th Delivery Address
          th Expires on
        for booking in bookings
          tr
            td #{booking.plan}
            td #{booking.user}
            td #{booking.id}
            td #{String(booking.timeOfDel.getHours()).padStart(2,0)}:#{String(booking.timeOfDel.getMinutes()).padStart(2,0)}
            td #{booking.delAddress}
            td #{booking.exp.toDateString()}
  else
    h3 Sorry no resuls found !